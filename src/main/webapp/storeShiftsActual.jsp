<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><div class="jumbotron">    <div class="container">        <h2>Store Shifts Needed</h2>    </div></div><div class="container">    <div class="row">        <div class="addNewStore">            <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#myModal">Add New Shift</button>        </div>    </div>    <div class="row">        <div class="table-responsive">            <table class="table table-hover">                <thead>                <tr>                    <th>Store Name</th>                    <th>Work Day</th>                    <th>Start Time</th>                    <th>End Time</th>                    <th>Job Title</th>                </tr>                </thead>                <tbody>                <c:forEach items="${allShifts}" var="shift">                    <tr>                        <td>${shift.storeId.storeName}</td>                        <td>${shift.workDay}</td>                        <td>${shift.startTime}</td>                        <td>${shift.endTime}</td>                        <td>${shift.jobTitle}</td>                    </tr>                </c:forEach>                </tbody>            </table>        </div>    </div></div>